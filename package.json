{
  "name": "tax_management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "preview": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve --mode preview",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build && node build.js"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.8",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "echarts": "^5.1.2",
    "element-ui": "^2.15.14",
    "firebase": "^9.6.9",
    "hls.js": "^1.4.0",
    "jszip": "^3.7.1",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "md5": "^2.3.0",
    "sortablejs": "^1.14.0",
    "tinymce": "^5.9.2",
    "videojs-contrib-hls": "^5.15.0",
    "vue": "^2.6.11",
    "vue-router": "^3.0.7",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-inner-declarations": "off",
      "invalid-first-character-of-tag-name": "off",
      "no-constant-condition": "off",
      "no-unused-vars": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
